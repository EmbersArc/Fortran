    "version": "0.1.0",
    "command": "gfortran",
    "isShellCommand": true,
    "echoCommand": false,
    "args": [

        "-g",
        "-Wall",
        "-fbounds-check",
        "-fbacktrace",
        "-ffpe-trap=zero,overflow,underflow,invalid",

        "computeStep.f90",
        "computeNewton.f90",
        "newton.f90",

        "-o", "newton"
        
        ],
    "showOutput": "always"
}